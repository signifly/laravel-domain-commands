{
    "name": "signifly/laravel-domain-commands",
    "description": "Artisan commands to Laravel apps that utilizes Domain Driven Design.",
    "homepage": "https://github.com/signifly/laravel-domain-commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@signifly.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\Console\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Signifly\\Console\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Signifly\\Console\\DomainServiceProvider"
            ]
        }
    }
}
